# Database Configuration (Railway PostgreSQL)
DATABASE_URL=postgresql://username:password@host:port/database

# SaaS API Admin Master Key (for management endpoints)
# Use X-Admin-Key header with this value to create orgs, teams, model groups, etc.
# IMPORTANT: Change this in production!
MASTER_KEY=sk-admin-your-super-secure-admin-key-here

# LiteLLM Master Key (for LiteLLM proxy authentication)
# This is separate from the SaaS API admin key
LITELLM_MASTER_KEY=sk-litellm-your-super-secure-litellm-key-here

# LiteLLM Proxy URL (for SaaS API to connect to LiteLLM)
# Local: http://localhost:8002
# Railway: http://litellm-proxy.railway.internal:4000
LITELLM_PROXY_URL=http://localhost:8002

# LiteLLM Database Storage (persist models/teams/keys in database)
STORE_MODEL_IN_DB=True

# API Keys for LLM Providers
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Redis Configuration (required for LiteLLM caching and rate limiting)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_URL=redis://localhost:6379

# Server Settings
HOST=0.0.0.0
PORT=8000
WORKERS=1
ENVIRONMENT=production
DEBUG=false
